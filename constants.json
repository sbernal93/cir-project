{
  "db_file_paths": {
    "database": "data/flights_db.pkl",
    "dict": "data/flights_dict.pkl",
    "user_goals": "data/flight_user_goal.pkl"
  },
  "run": {
    "usersim": false,
    "warmup_mem": 1000,
    "num_ep_run": 40000,
    "train_freq": 100,
    "max_round_num": 20,
    "success_rate_threshold": 0.3
  },
  "agent": {
    "save_weights_file_path": "weights/flights_model.h5",
    "load_weights_file_path": "weights/flights_model.h5",
    "vanilla": false,
    "learning_rate": 1e-4,
    "batch_size": 16,
    "dqn_hidden_size": 80,
    "epsilon_init": 0.2,
    "gamma": 0.6,
    "max_mem_size": 500000,
    "activation1": "relu",
    "activation2": "linear",
    "loss": "mse"
  },
  "emc": {
    "slot_error_mode": 0,
    "slot_error_prob": 0.05,
    "intent_error_prob": 0.0
  }
}
